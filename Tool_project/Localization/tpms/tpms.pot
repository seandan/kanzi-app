msgstr ""

msgctxt "sl2"
msgid "请到维修店更换新的传感器"
msgstr ""

msgctxt "know"
msgid "知道了"
msgstr ""

msgctxt "getting"
msgid "数据信息获取中，请稍后......"
msgstr ""

msgctxt "getting2"
msgid "（车速大于30km/h，轮胎信息逐步显示！）"
msgstr ""

msgctxt "false"
msgid "控制器未连接或者控制器损坏！"
msgstr ""

msgctxt "studying"
msgid "胎压监测系统正在进行学习模式！"
msgstr ""

msgctxt "studyfalse"
